var

fn f2() : bool
begin
  let x : bool = f1() in
  x
end

fn f1() : bool
begin
  let x : bool = f2() in
  x
end

fn f3() : bool
begin
  let x : bool = f1() in
  let y : bool = x & True in
  let z : bool = y & True in
  z
end


automaton P

  loc s0: invariant True
  when f1() & f3() goto lend;

  accepting loc lend : invariant True

end


init := {
  discrete =
    loc[P]:=s0
  ;

}
